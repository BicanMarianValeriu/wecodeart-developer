{
  "env": {
      "browser": true,
      "es6": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
      "ecmaVersion": 9,
      "sourceType": "module",
      "ecmaFeatures": {
          "experimentalObjectRestSpread": true
      }
  },
  "rules": {
      "semi": 2,
      "indent": [
          "error",
          "tab"
      ]
  }
}